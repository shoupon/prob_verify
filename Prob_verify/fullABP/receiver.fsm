NAME	receiver;
INPUTS		fcomch?m0, fcomch?m1;
OUTPUTS		rcomch!ack1, rcomch!ack0, service!m;
INITIAL_STATE	S0;
OTHER_STATES	S1;
TRANSITIONS	S0	: S1 WHEN (fcomch?m0,rcomch!ack*service!m) COST 1 HIGH
			: S0 WHEN (fcomch?m1,rcomch!ack) COST 1 HIGH,
		S1	: S1 WHEN (fcomch?m0,rcomch!ack) COST 1 HIGH
			: S0 WHEN (fcomch?m1,rcomch!ack*service!m) COST 1 HIGH		
EOF
